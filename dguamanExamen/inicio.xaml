<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dguamanExamen.inicio">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="inicio" FontSize="24" HorizontalTextAlignment="Center"/>
            <Image Source="https://jorzgz0iptpc.cdn.shift8web.com/wp-content/uploads/2018/01/LOGO-UISRAEL-768x209.png"/>
             <Entry x:Name="txtDato" Placeholder="usuario"/>
            <Entry x:Name="txtDatodos" Placeholder="Clave" IsPassword="true"/>
            <Button x:Name="btnAbrir" Text="Iniciar sesion" Clicked="btnAbrir_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>